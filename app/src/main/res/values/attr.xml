<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="BottomView">
        <attr name="bottom_icon" format="reference"/>
        <attr name="bottom_color" format="color"/>
        <attr name="bottom_text" format="string"/>
        <attr name="bottom_textSize" format="dimension"/>
    </declare-styleable>

    <declare-styleable name="CircleImageView">
        <attr name="civ_border_width" format="dimension"/>
        <attr name="civ_border_color" format="color"/>
        <attr name="civ_border_overlay" format="boolean"/>
        <attr name="civ_fill_color" format="color"/>
    </declare-styleable>

    <declare-styleable name="PublishButtonView">
        <attr name="publish_text" format="string"/>
    </declare-styleable>
</resources>